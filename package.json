{
  "name": "skinapp",
  "displayName": "Skin App",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "apk": "npm run android:build && npm run android:generate && npm run android:open",
    "android:build": " react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "android:generate": "cd android && gradlew assembleRelease",
    "android:open": "cd android/app/build/outputs/apk/release && start ."
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.6.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-firebase/app": "^19.1.1",
    "@react-native-google-signin/google-signin": "^10.1.2",
    "@react-navigation/bottom-tabs": "^6.5.19",
    "@react-navigation/native": "^6.1.16",
    "@react-navigation/stack": "^6.3.28",
    "axios": "^1.6.8",
    "npm-run-all": "^4.1.5",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "^2.16.0",
    "react-native-image-crop-picker": "^0.40.3",
    "react-native-image-picker": "^7.1.1",
    "react-native-image-resizer": "^1.4.5",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "^3.8.1",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-vector-icons": "^10.0.3",
    "reanimated-bottom-sheet": "^1.0.0-alpha.22"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-native-dotenv": "^0.2.2",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
